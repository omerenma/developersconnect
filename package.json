  },
  "homepage": "https://github.com/omerenma/developersconnect#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "create-react-app": "^2.1.1",
    "express": "^4.16.4",
    "front": "0.0.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.7.5",
